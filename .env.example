# Private key of the deployer account (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Sepolia RPC URL (Infura, Alchemy, or other provider)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# OR: https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: IPT Token address (if deploying Excell separately)
# IPT_TOKEN_ADDRESS=0x...
